
<table class ="table">
  <tr>
    <th>image</th>
    <th>name</th>
  </tr>
  <% users.each do |user| %>
  <tr>
    <td><%=image_tag user.get_profile_image %></td>
    <td><%=user.name %></td>
    <td>フォロー数<%=user.followings.count %></td>
    <td>フォロワー数<%=user.followers.count %></td>
    <td><%= link_to "Show",user_path(user)%></td>
  <% end %>
  </tr>
</table>
